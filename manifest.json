{
  "manifest_version": 3,
  "name": "DoggoReader",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "description": "Summarizes the content of the current website you are on. Great for summarizing long and complex articles.",
  "permissions": ["activeTab", "scripting", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_popup_size": {
      "width": 800,
      "height": 600
    }
  },
  "key": ""
}
